{
  "name": "chinese-text-extractor",
  "version": "1.0.0",
  "description": "一个用于提取TypeScript、JavaScript和Prefab文件中中文字符串的工具",
  "main": "dist/index.js",
  "bin": {
    "extract-chinese": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "extract": "npm run build && node dist/cli.js",
    "test": "npm run build && node dist/test.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "chinese",
    "text",
    "extraction",
    "typescript",
    "javascript",
    "prefab",
    "i18n",
    "internationalization",
    "cocos",
    "游戏开发",
    "中文提取"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/chinese-text-extractor.git"
  },
  "homepage": "https://github.com/yourusername/chinese-text-extractor#readme",
  "bugs": {
    "url": "https://github.com/yourusername/chinese-text-extractor/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pako": "^2.0.3",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.3",
    "@babel/traverse": "^7.23.3",
    "@types/babel__traverse": "^7.20.4",
    "pako": "^2.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
